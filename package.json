{
  "name": "@flooz-link/plugin-speech-tts",
  "version": "0.1.10",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "scripts",
    "package.json",
    "LICENSE",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@deepgram/sdk": "^3.9.0",
    "@elizaos/core": "^0.25.9",
    "echogarden": "2.0.14",
    "formdata-node": "^6.0.3",
    "nodejs-whisper": "0.2.6",
    "openai": "^4.81.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/eslintrc": "3.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.12.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.2.6",
    "tsup": "8.4.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "lint": "eslint --fix  --cache .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "OPENAI_API_KEY": {
        "type": "string"
      },
      "OPENAI_API_URL": {
        "type": "string",
        "default": "https://api.openai.com",
        "optional": true
      },
      "ELEVENLABS_XI_API_URL": {
        "type": "string",
        "default": "https://api.elevenlabs.io/v1",
        "optional": true
      },
      "ELEVENLABS_XI_API_KEY": {
        "type": "string",
        "optional": true
      },
      "ELEVENLABS_MODEL_ID": {
        "type": "string",
        "default": "eleven_monolingual_v1",
        "optional": true
      },
      "ELEVENLABS_VOICE_ID": {
        "type": "string",
        "optional": true
      },
      "ELEVENLABS_VOICE_STABILITY": {
        "type": "string",
        "default": "0.5",
        "optional": true
      },
      "ELEVENLABS_VOICE_SIMILARITY_BOOST": {
        "type": "string",
        "default": "0.75",
        "optional": true
      },
      "ELEVENLABS_VOICE_STYLE": {
        "type": "string",
        "default": "0",
        "optional": true
      },
      "ELEVENLABS_VOICE_USE_SPEAKER_BOOST": {
        "type": "string",
        "default": "true",
        "optional": true
      },
      "ELEVENLABS_OPTIMIZE_STREAMING_LATENCY": {
        "type": "string",
        "default": "0",
        "optional": true
      },
      "ELEVENLABS_OUTPUT_FORMAT": {
        "type": "string",
        "default": "pcm_16000",
        "optional": true
      },
      "VITS_VOICE": {
        "type": "string",
        "optional": true
      },
      "VITS_MODEL": {
        "type": "string",
        "optional": true
      }
    }
  }
}
